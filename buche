  // ğŸ›‹ï¸ Amueblar casa si estÃ¡ asignada, visible y sin personas cerca
    if (_edificio getVariable ["BUCHEDEG_amueblado", 0] == 1 && {!(isObjectHidden _edificio)} && {count (_edificio nearEntities ["Man", 7]) == 0}) then {
        _items = _edificio getVariable ["BUCHEDEG_muebles", []];
        _edificio setVariable ["BUCHEDEG_amueblado", 2, true];
        _dir = getDir _edificio;
        _spawned = [];
        
        // ğŸ”„ CreaciÃ³n de muebles sincronizada
        if (isServer) then {
            {
                if (random 1 > 0.25) then {
                    _item = _x select 0;
                    _offset = _x select 1;
                    _angulo = _x select 2;
                    _pos = _edificio modelToWorld _offset;
                    
                    // Usar createVehicle para sincronizaciÃ³n completa entre clientes
                    _mueble = createVehicle [_item, [0,0,0], [], 0, "CAN_COLLIDE"];
                    _mueble setPosATL _pos;
                    _mueble setDir (_dir - _angulo);
                    _mueble enableSimulation false; // Deshabilitar fÃ­sica para mejor rendimiento
                    _mueble allowDamage false; // Evitar que se daÃ±e
                    
                    // Agregar a la lista de muebles para esta casa
                    _spawned pushBack _mueble;
                    
                    // PequeÃ±a pausa para evitar lagspikes
                    sleep 0.05;
                };
            } forEach _items;
            
            // ğŸ’¾ Guardar IDs para JIP y sincronizaciÃ³n
            _idsMuebles = [];
            {
                _idsMuebles pushBack (netId _x);
            } forEach _spawned;
            
            _edificio setVariable ["BUCHEDEG_muebles_ids", _idsMuebles, true];
            
            // ğŸ†• AÃ±adir event handler para eliminaciÃ³n por Zeus u otros medios
            _edificio addEventHandler ["Deleted", {
                params ["_edificio"];
                [_edificio] call BUCHEDEG_fnc_manejarEliminacion;
            }];
            
            // ğŸ†• AÃ±adir event handler para daÃ±o y destrucciÃ³n
_edificio addEventHandler ["Hit", {
    params ["_edificio", "_causedBy", "_damage", "_instigator"];
    
    // Si el daÃ±o es grande (>0.9) y el edificio ya tiene muebles
    if (_damage > 0.9 && {_edificio getVariable ["BUCHEDEG_amueblado", 0] == 2}) then {
        // Llamar a la funciÃ³n de manejo de destrucciÃ³n
        [_edificio] call BUCHEDEG_fnc_manejarDestruccion;
    };
}];
            
            _edificio addEventHandler ["Killed", {
                params ["_edificio", "_killer", "_instigator", "_useEffects"];
                
                // Llamar a la funciÃ³n de manejo de destrucciÃ³n
                [_edificio] call BUCHEDEG_fnc_manejarDestruccion;
            }];
        };
        
        // AÃ±adir casa a la lista global
        BUCHEDEG_muebles_casas pushBack _edificio;
        ["Casa amueblada con Ã©xito"] call BUCHEDEG_fnc_log;
    };
};

// ğŸ”„ FunciÃ³n para actualizar muebles cuando un jugador se une
BUCHEDEG_fnc_sincronizarJIP = {
    {
        private _casa = _x;
        private _estado = _casa getVariable ["BUCHEDEG_amueblado", 0];
        
        // Solo sincronizar casas ya amuebladas
        if (_estado == 2) then {
            private _ids = _casa getVariable ["BUCHEDEG_muebles_ids", []];
            private _muebles = [];
            
            // Convertir IDs en objetos
            {
                private _mueble = objectFromNetId _x;
                if (!isNull _mueble) then {
                    _muebles pushBack _mueble;
                };
            } forEach _ids;
            
            // Actualizar variable local
            _casa setVariable ["BUCHEDEG_spawned", _muebles];
        };
    } forEach (nearestObjects [player, ["House"], BUCHEDEG_muebles_radio * 1.5]);
};

// âŒ FunciÃ³n para eliminar muebles de una casa
BUCHEDEG_fnc_eliminarMuebles = {
    params ["_edificio"];
    
    if (isServer) then {
        private _ids = _edificio getVariable ["BUCHEDEG_muebles_ids", []];
        
        {
            private _mueble = objectFromNetId _x;
            if (!isNull _mueble) then {
                deleteVehicle _mueble;
            };
        } forEach _ids;
        
        // Resetear variables
        _edificio setVariable ["BUCHEDEG_amueblado", 1, true];
        _edificio setVariable ["BUCHEDEG_muebles_ids", [], true];
    };
};

// ğŸ’¥ FunciÃ³n para manejar destrucciÃ³n de edificios
// ğŸ’¥ FunciÃ³n para manejar destrucciÃ³n de edificios
BUCHEDEG_fnc_manejarDestruccion = {
    params ["_edificio"];
    
    // Verificar si tiene muebles
    if (_edificio getVariable ["BUCHEDEG_amueblado", 0] != 2) exitWith {};
    
    // Obtener muebles
    private _ids = _edificio getVariable ["BUCHEDEG_muebles_ids", []];
    private _muebles = [];
    
    // Convertir IDs en objetos
    {
        private _mueble = objectFromNetId _x;
        if (!isNull _mueble) then {
            _muebles pushBack _mueble;
        };
    } forEach _ids;
    
    // Si no hay jugadores cerca, no hacer nada con los muebles
    if (({_x distance _edificio < 100} count allPlayers) == 0) then {
        // Los muebles se quedan donde estÃ¡n
        ["Edificio destruido: muebles permanecen intactos (sin jugadores cerca)"] call BUCHEDEG_fnc_log;
    } else {
        // Efecto de explosiÃ³n para muebles (solo servidor)
        if (isServer) then {
            // Crear una pequeÃ±a explosiÃ³n para el efecto
            private _pos = getPosATL _edificio;
            "SmallSecondary" createVehicle _pos;
            
            // Hacer que los muebles "salgan volando" pero no se eliminen
            {
                if (!isNull _x) then {
                    // Habilitar simulaciÃ³n fÃ­sica
                    _x enableSimulation true;
                    _x allowDamage true;
                    
                    // Aplicar fuerza aleatoria
                    private _dir = random 360;
                    private _fuerza = 5 + random 10;
                    private _altura = 2 + random 3;
                    
                    // Calcular vector de velocidad
                    private _vel = [
                        sin(_dir) * _fuerza, 
                        cos(_dir) * _fuerza, 
                        _altura
                    ];
                    
                    // Aplicar velocidad
                    _x setVelocity _vel;
                    
                    // RotaciÃ³n aleatoria
                    [_x, random 360] remoteExec ["setDir", 0];
                    
                    // No programamos su eliminaciÃ³n, dejamos que permanezcan en el mundo
                };
            } forEach _muebles;
            ["Edificio destruido: muebles dispersados (jugadores cerca)"] call BUCHEDEG_fnc_log;
        };
    };
    
    // Marcar el edificio para evitar duplicaciones
    _edificio setVariable ["BUCHEDEG_amueblado", -2, true]; // -2: destruido
    
    // Eliminar de la lista global
    BUCHEDEG_muebles_casas = BUCHEDEG_muebles_casas - [_edificio];
    
    // No eliminamos el arreglo de IDs para mantener la referencia a los muebles
    // _edificio setVariable ["BUCHEDEG_muebles_ids", nil, true];
};

// ğŸ”® FunciÃ³n para manejar eliminaciÃ³n por Zeus u otros medios
BUCHEDEG_fnc_manejarEliminacion = {
    params ["_edificio"];
    
    if (isNull _edificio) exitWith {};
    
    // Verificar si el edificio tenÃ­a muebles
    if (_edificio getVariable ["BUCHEDEG_amueblado", 0] == 2) then {
        // Obtener IDs de muebles
        private _ids = _edificio getVariable ["BUCHEDEG_muebles_ids", []];
        
        // Eliminar todos los muebles
        {
            private _mueble = objectFromNetId _x;
            if (!isNull _mueble) then {
                deleteVehicle _mueble;
            };
        } forEach _ids;
        
        // Eliminar de la lista global
        BUCHEDEG_muebles_casas = BUCHEDEG_muebles_casas - [_edificio];
        ["Edificio eliminado por Zeus: muebles limpiados"] call BUCHEDEG_fnc_log;
    };
};

// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
// ğŸ”„ BUCLE PRINCIPAL
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
sleep 5; // Breve pausa inicial para asegurar inicializaciÃ³n correcta

// ğŸ”„ Evento para jugadores que se unen tarde (JIP)
if (!isServer && hasInterface) then {
    [] spawn {
        sleep 8; // Dar tiempo para que se sincronicen las variables
        [] call BUCHEDEG_fnc_sincronizarJIP;
    };
};

// ğŸ”„ Bucle principal del sistema
[] spawn {
    private ["_edificios", "_edificio"];
    
    while {true} do {
        // ğŸ” Escanear casas para amueblar
        if (BUCHEDEG_muebles_activo && {player == vehicle player} && {player distance BUCHEDEG_muebles_ultimapos > (BUCHEDEG_muebles_radio / 2)}) then {
            BUCHEDEG_muebles_ultimapos = position player;
            _edificios = player nearObjects ["House", BUCHEDEG_muebles_radio];
            
            // Procesar cada edificio cercano
            {
                [_x] spawn BUCHEDEG_fnc_amueblarCasa;
            } forEach _edificios;
            
            // ğŸ§¹ Eliminar muebles de casas distantes
            for "_i" from 0 to count BUCHEDEG_muebles_casas - 1 do {
                _edificio = BUCHEDEG_muebles_casas select _i;
                
                if (!isNull _edificio && {_edificio distance player > BUCHEDEG_muebles_radio}) then {
                    [_edificio] call BUCHEDEG_fnc_eliminarMuebles;
                    BUCHEDEG_muebles_casas set [_i, objNull];
                };
            };
            
            // Limpiar array de casas
            BUCHEDEG_muebles_casas = BUCHEDEG_muebles_casas - [objNull];
        };
        
        // â³ Esperar hasta el siguiente escaneo
        sleep BUCHEDEG_muebles_tiempo;
    };
};

// ğŸ” Detector periÃ³dico de edificios eliminados (respaldo para Zeus)
if (isServer) then {
    [] spawn {
        while {true} do {
            // Comprobar edificios en la lista global
            for "_i" from 0 to (count BUCHEDEG_muebles_casas - 1) do {
                if (_i < count BUCHEDEG_muebles_casas) then {
                    private _casa = BUCHEDEG_muebles_casas select _i;
                    
                    // Verificar si el edificio ya no existe
                    if (isNull _casa) then {
                        // Ya no existe, asegurar limpieza de muebles
                        private _ids = _casa getVariable ["BUCHEDEG_muebles_ids", []];
                        
                        {
                            private _mueble = objectFromNetId _x;
                            if (!isNull _mueble) then {
                                deleteVehicle _mueble;
                            };
                        } forEach _ids;
                        
                        // Marcar para eliminaciÃ³n
                        BUCHEDEG_muebles_casas set [_i, objNull];
                        ["Detector periÃ³dico: edificio eliminado detectado"] call BUCHEDEG_fnc_log;
                    };
                };
            };
            
            // Limpiar lista
            BUCHEDEG_muebles_casas = BUCHEDEG_muebles_casas - [objNull];
            
            // Esperar antes de la prÃ³xima comprobaciÃ³n
            sleep 15;
        };
    };
};

["Sistema de amueblado iniciado correctamente"] call BUCHEDEG_fnc_log;
